[user]
	email = hugemerge@gmail.com
	name = sushidesu
[init]
	defaultBranch = main
[pull]
	ff = only
[core]
	editor = vim -c \"set fenc=utf-8\"
	ignorecase = false
	quotepath = false
[advice]
	detachedHead = false
[alias]
	alias = config --get-regexp ^alias\\.
	stu = stash -u
	stp = stash pop
	stl = stash list
	lo = log --oneline -12 --reverse
	logn = log --name-status --oneline -5
	ss = status
	ca = commit --amend
	can = commit --amend --no-edit
	fp = fetch --prune
	a = add -A
	c = commit
	cm = commit -m
	ac = commit -a
	pl = pull
	ps = push
	sw = switch
	swc = switch -c
	b = branch
	ba = branch -a
	bd = branch -d
	bdd = branch -D

[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
